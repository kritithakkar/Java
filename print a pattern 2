import  java.util.Scanner;
public class pattern2 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=i;j++)  // i=1 needs 1 star  i=2 needs 2 star
            {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
