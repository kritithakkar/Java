import java.util.Scanner;

public class whileQues3 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i=1;
        int f=1;
        while(i<=n)
        {
           f*=i;
           i++;
        }
        System.out.println(f);
    }
}
